<html lang="en" class="no-js">
  <!-- Mirrored from haintheme.com/demo/html/bustter/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Mon, 03 Jun 2019 15:14:19 GMT -->
  <head>
    <!-- Basic need -->
    <title><%= title %></title>
    <meta charset="UTF-8" />
    <meta name="description" content="An Anime Streaming Website" />
    <meta name="author" content="Project Sakura" />
    <link rel="profile" href="#" />

    <!--Google Font-->
    <link
      rel="stylesheet"
      href="https://fonts.googleapis.com/css?family=Dosis:400,700,500|Nunito:300,400,600"
    />
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css"
    />

    <!-- Mobile specific meta -->
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="format-detection" content="telephone-no" />

    <!-- Favicon -->
    <link rel="shortcut icon" href="images/favicon/favicon.ico" />

    <!-- CSS files -->
    <link rel="stylesheet" href="stylesheets/plugins.css" />
    <link rel="stylesheet" href="stylesheets/style.css" />

    <style>
      #status {
        display: none;
      }
      #search_result::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        border-radius: 10px;
      }
      #search_result::-webkit-scrollbar {
        width: 12px;
      }
      #search_result::-webkit-scrollbar-thumb {
        border-radius: 10px;
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        background-color: #dd003f;
      }
      body::-webkit-scrollbar-track {
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        border-radius: 10px;
        background-color: #020d18;
      }

      body::-webkit-scrollbar {
        width: 12px;
        background-color: #020d18;
      }

      body::-webkit-scrollbar-thumb {
        border-radius: 10px;
        -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3);
        background-color: #dd003f;
      }

      .logo {
        border-radius: 50%;
        -webkit-animation: rotation 5s infinite
          cubic-bezier(0.075, 0.82, 0.165, 1);
      }

      @-webkit-keyframes rotation {
        from {
          -webkit-transform: rotate(0deg);
        }
        to {
          -webkit-transform: rotate(359deg);
          -webkit-box-shadow: 0px 0px 25px 5px rgba(221, 0, 63, 1);
          -moz-box-shadow: 0px 0px 25px 5px rgba(221, 0, 63, 1);
          box-shadow: 0px 0px 25px 5px rgba(221, 0, 63, 1);
        }
      }
      @media (max-width: 768px) {
        .logo {
          animation: none;
          box-shadow: none;
          border-radius: none;
        }
      }

      .ongoing ul {
        display: none !important;
      }
      @media (max-width: 768px) {
        .oolla .slick-dots {
          display: none !important;
        }
        .oolla .slick-slide {
          width: 180px !important;
        }
        .oolla .tab {
          height: 235px !important;
        }
      }
      @media (max-width: 425px) {
        .oolla .slick-dots {
          display: none !important;
        }
        .oolla .slick-slide {
          width: 190px !important;
        }
        .slick-dots {
          text-align: center !important;
        }
      }
      @media (max-width: 375px) {
        .oolla .slick-dots {
          display: none !important;
        }
        .oolla .slick-slide {
          width: 165px !important;
        }
        .slick-dots {
          text-align: center !important;
        }
      }
      @media (max-width: 360px) {
        .oolla .slick-dots {
          display: none !important;
        }
        .oolla .slick-slide {
          width: 157px !important;
        }
        .slick-dots {
          text-align: center !important;
        }
      }
    </style>
  </head>
  <body>
    <!--preloading-->
    <div id="preloader">
      <img class="logo" src="images/logo3.ico" alt="" width="119" height="58" />

      <div class="text-center" style="color: #fff; margin-top: 60px">
        <h1>Mirai</h1>
      </div>
    </div>


    <!-- BEGIN | Header -->
    <header class="ht-header">
      <div class="container">
        <nav class="navbar navbar-default navbar-custom">
          <!-- Brand and toggle get grouped for better mobile display -->
          <div class="navbar-header logo">
            <div
              class="navbar-toggle"
              data-toggle="collapse"
              data-target="#bs-example-navbar-collapse-1"
            >
              <span class="sr-only">Toggle navigation</span>
              <div id="nav-icon1">
                <span></span>
                <span></span>
                <span></span>
              </div>
            </div>
            <a href="/"
              ><img class="logo" src="images/logo3.ico" />
              <span
                class="whoosh"
                style="
                  font-family: 'Dosis', sans-serif;
                  font-size: 36px;
                  color: #ffffff;
                  font-weight: 700;
                  display: none;
                  text-transform: none;
                "
                >Mirai</span
              ></a
            >
          </div>
          <!-- Collect the nav links, forms, and other content for toggling -->
          <div
            class="collapse navbar-collapse flex-parent"
            id="bs-example-navbar-collapse-1"
          >
            <ul class="nav navbar-nav flex-child-menu menu-left">
              <li class="hidden">
                <a href="#page-top"></a>
              </li>

              <li>
                <a
                  class="animelist"
                  link="https://www.gogoanime1.com/home/anime-list"
                  style="font-size: 13px"
                >
                  Anime List
                </a>
              </li>
              <li>
                <a
                  class="latestManga"
                  href="loadlatest"
                  style="font-size: 13px"
                >
                  Latest Manga
                </a>
              </li>
              
            </ul>
          </div>
        </nav>

        <div class="top-search">
          <select id="selector-val">
            <option value="anime" selected>Anime</option>
            <option value="manga">Manga</option>
          </select>
          <input
            type="text"
            id="searchInput"
            placeholder="Search for an Anime, that you would like to search"
          />
        </div>
        <div
          id="search_result"
          style="
            display: none;
            z-index: 100000;
            padding: 20px;
            overflow-y: auto;
            overflow-x: hidden;
            color: #fff;
            background: #233a50;
            max-height: 400px;
            border: 4px solid #020d18;
          "
        >
          <ul style="overflow: hidden"></ul>
        </div>
      </div>
    </header>
  </body>
</html>
